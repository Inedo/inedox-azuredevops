    <OutputType>Library</OutputType>
    <RootNamespace>Inedo.Extensions.Git</RootNamespace>
    <AssemblyName>Git</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <LangVersion>latest</LangVersion>
